{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Fluent",
      "program": "${workspaceFolder}/dist/debug-fluent.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Decorator",
      "program": "${workspaceFolder}/node_modules/ava/profile.js",
      "args": [
        "--serial",
        "${workspaceFolder}/dist/e2e/decorators.spec.js"
      ],
      "skipFiles": [
        "<node_internals>/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Test",
      "program": "${workspaceFolder}/node_modules/ava/profile.js",
      "args": [
        "--serial",
        "${workspaceFolder}/dist/lib/services/activator.spec.js"
      ],
      "skipFiles": [
        "<node_internals>/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "attach",
      "name": "Attach by Process ID",
      "processId": "${command:PickProcess}"
    }
  ]
}